.tabView {
  width: 100%;
  height: 40px;
  background-color: white;
  display: flex;
  align-items: center;
  text-align: center;
}

.tabCheckedView {
  flex-grow: 1;
  font-size: 32rpx;
  color: #4d4d4d;
  height: 30px;
  border-bottom: 2px solid #09f;
  padding-top: 7px;
}

.tabUnCheckedView {
  flex-grow: 1;
  font-size: 32rpx;
  color: #4d4d4d;
}

